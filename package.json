{
  "name": "formik-final-form",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "format": "prettier --write 'src/**/*.{css,scss,ts,tsx}'",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "prepare": "husky install",
    "start": "HTTPS=true BROWSER=none react-scripts start",
    "test": "react-scripts test"
  },
  "engines": {
    "node": ">=12.17.0"
  },
  "dependencies": {
    "final-form": "4.20.2",
    "formik": "2.2.9",
    "fp-ts": "2.10.5",
    "node-sass": "6.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-final-form": "6.5.3",
    "react-scripts": "4.0.3",
    "typescript": "4.3.4",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/node": "15.12.4",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@types/yup": "0.29.11",
    "eslint": "7.29.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
